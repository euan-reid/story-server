    "version": "2.0.0",
    "tasks": [
        {
            "label": "Export Emulator Data to Tests",
            "detail": "Saves the current state of the emulator to tests/data",
            "type": "shell",
            "command": "bash ${workspaceFolder}/.vscode/exportEmulatorToTestData.sh",
            "problemMatcher": []
        },
        {
            "label": "Import Data to Emulator",
            "detail": "Loads the contents of tests/data to the emulator",
            "type": "shell",
            "command": "bash ${workspaceFolder}/.vscode/importTestDataToEmulator.sh",
            "problemMatcher": []
        }
    ]
}
